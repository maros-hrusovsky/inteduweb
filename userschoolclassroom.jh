entity MyUser {
	
}
entity School {
	name String
}

entity Classroom {
	name String
}

relationship ManyToMany {
	Classroom{users(firstname)} to MyUser{classrooms(name)}
}

relationship OneToMany {
	School{classrooms(name)} to Classroom
}
relationship OneToMany {
	School{users(firstname)} to MyUser
}
relationship OneToOne {
	User to MyUser
}